<%- include ("partials/header") -%>
  <h1>Home</h1>
  <p> <%=homeStartingContent%> </p>

<% posts.forEach((post)=>{%>
 <h2> <%= post.postTitle %> </h2>
 <% const length = 100; %>
    <% let postText = post.postBody.substring(0, Math.min(post.postBody.length, length));%>
     <% if(postText.length === length) postText += " ..."; %>
  <p> <%= postText %> <a href="posts/<%= post.postTitle%>">Read More</a> </p>
 <% }); %>
<%- include  ("partials/footer")-%>
